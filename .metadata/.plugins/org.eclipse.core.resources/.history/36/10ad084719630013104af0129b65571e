package br.com.xti;

import br.com.xti.*;

public class ContaTest {

	public static void main(String[] args) {
		Conta conta = new Conta();
		conta.cliente = "Ricardo";
		conta.saldo = 10000.00;
		conta.exibeSaldo();
		
		Conta destino = new Conta();
		destino.cliente = "Lawrence";
		destino.saldo = 8000.00;
		destino.exibeSaldo();
	}

}
